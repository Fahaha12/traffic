# Flask配置
SECRET_KEY=your-secret-key-here
JWT_SECRET_KEY=your-jwt-secret-key-here

# 数据库配置
DATABASE_URL=sqlite:///traffic_monitor.db
# 或者使用PostgreSQL
# DATABASE_URL=postgresql://username:password@localhost:5432/traffic_monitor
# 或者使用MySQL
# DATABASE_URL=mysql://username:password@localhost:3306/traffic_monitor

# Redis配置
REDIS_URL=redis://localhost:6379/0

# 文件上传配置
UPLOAD_FOLDER=uploads
MAX_CONTENT_LENGTH=104857600  # 100MB

# AI模型配置
MODEL_PATH=models
DETECTION_CONFIDENCE=0.5
TRACKING_MAX_DISAPPEARED=30

# 开发配置
FLASK_ENV=development
FLASK_DEBUG=True

# 生产配置
# FLASK_ENV=production
# FLASK_DEBUG=False
